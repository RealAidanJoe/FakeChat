                </ImageView>
            </children>
        </VBox>
        <VBox alignment="CENTER_LEFT" layoutX="65.0" layoutY="12.5" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="65.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="0.0">
