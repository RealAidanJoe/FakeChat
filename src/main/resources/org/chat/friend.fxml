                <Label fx:id="nameLab" text="安大微教务">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Label>
                <Label fx:id="chatLab" text="学校通知" textFill="GREY" />
            </children>
        </VBox>
        <Circle fx:id="hasNewMsgCir" fill="RED" layoutX="225.0" layoutY="13.0" radius="5.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" visible="false" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0" />
